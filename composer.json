{
    "name": "ruwork/polyfill-form-dti",
    "description": "Symfony Form polyfill adding input=datetime_immutable to date types.",
    "type": "symfony-bundle",
    "keywords": ["symfony", "form", "polyfill"],
    "license": "MIT",
    "authors": [
        {"name": "Valentin Udaltsov", "email": "udaltsov.valentin@gmail.com"}
    ],
    "require": {
        "php": ">=5.5",
        "symfony/form": "^2.8 || ^3.0 || ^4.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.4.8",
        "symfony/phpunit-bridge": "^2.8 || ^3.0 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "Ruwork\\PolyfillFormDTI\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        }
    }
}
